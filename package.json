{
  "name": "gd-project",
  "type": "module",
  "version": "2.1.0",
  "private": true,
  "packageManager": "pnpm@7.26.0",
  "description": "A blazing fast web app for grade distribution analysis.",
  "author": "adibarra <github@adibarra.com> (https://adibarra.com)",
  "scripts": {
    "build": "pnpm -r build",
    "commit": "git-cz",
    "dev": "pnpm -r --parallel dev",
    "lint": "pnpm -r --parallel lint",
    "taze": "taze major -I -r"
  },
  "devDependencies": {
    "commitizen": "^4.3.0",
    "commitplease": "^3.2.0",
    "cz-conventional-changelog": "^3.3.0",
    "taze": "^0.8.5"
  },
  "commitplease": {
    "style": "angular"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  }
}
